{
  "#LoginPage": {
    ".page": {
      "backgroundColor":  { 
				"startColor": "#FFBB13",
				"endColor": "#AC603B",
				"direction": "DIAGONAL_RIGHT"
			}
    },
    ".title": {
        "visible": true,
        "multiline": false,
        "alpha": 1,
        "borderColor": "#FFFFFF",
        "borderWidth": 0,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDCENTER",
        "font": {
          "size": 30,
          "bold": false,
          "italic": false,
          "family": "CenturyGothic",
          "weight": "Regular"
        },
        "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "flexGrow": 1
        }
    },
    ".usernameIcon" : {
        "width": 30,
        "height": null,
        "visible": true,
        "imageFillType": "MIDLEFT",
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
        "alignSelf": "AUTO",
        "positionType": "RELATIVE",
        "image": "username.png"
    },
    ".usernameInput" : {
        "width": null,
        "height": 60,
        "visible": true,
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDLEFT",
        "font": {
          "size": 15,
          "bold": false,
          "italic": false,
          "family": "Avenir",
          "weight": "Book"
        },
        "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "flexGrow": 1
        }
    },
    ".divider": {
       "height":1,
        "width": null,
        "visible": true,
        "backgroundColor": "#FFFFFF",
        "alpha": 1,
        "direction": "INHERIT",
        "flexProps": {
          "alignSelf": "AUTO",
          "alignContent": "STRETCH",
          "alignItems": "STRETCH",
          "justifyContent": "FLEX_START",
          "flexWrap": "NOWRAP",
          "flexDirection": "COLUMN",
          "positionType": "RELATIVE",
          "marginBottom": 5
        }
    },
    ".passwordIcon" : {
        "width": 30,
        "height": null,
        "visible": true,
        "imageFillType": "MIDLEFT",
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
        "alignSelf": "AUTO",
        "positionType": "RELATIVE",
        "image": "password.png"
    },
    ".passwordInput" : {
        "width": null,
        "height": 60,
        "visible": true,
        "alpha": 1,
        "borderColor": "#000000",
        "borderWidth": 0,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDLEFT",
        "font": {
          "size": 15,
          "bold": false,
          "italic": false,
          "family": "Avenir",
          "weight": "Book"
        },
        "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "flexGrow": 1
        }
    },
    ".rememberMeTitle": {
        "width": null,
        "height": 30,
        "visible": true,
        "multiline": false,
        "alpha": 0.8,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDLEFT",
        "font": {
          "size": 12.8,
          "bold": false,
          "italic": false,
          "family": "Avenir",
          "weight": "Book"
        },
        "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "ios.shadowOpacity": 0
        }
    },
     ".fingerPrintTitle": {
        "width": null,
        "height": 30,
        "visible": true,
        "multiline": false,
        "alpha": 0.8,
        "textColor": "#FFFFFF",
        "textAlignment": "MIDLEFT",
        "font": {
          "size": 12.8,
          "bold": false,
          "italic": false,
          "family": "Avenir",
          "weight": "Book"
        },
        "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "ios.shadowOpacity": 0
        }
    },
    ".rememberMeSwitch": {
        "flexProps": {
            "flexGrow": 1,
            "alignSelf": "AUTO",
            "positionType": "RELATIVE"
        },
        "thumbOnColor": "#616DFC"
    },
    ".loginButton": {
      "touchEnabled": true,
      "backgroundColor": "rgba(0,0,0,0)",
      "height": 47,
      "width": null,
      "visible": true,
      "alpha": 1,
      "textColor": "#FFFFFF",
      "textAlignment": "MIDCENTER",
      "font":{
        "size": 16,
        "bold": false,
        "italic": false,
        "family": "Avenir",
        "weight": "Book"
      },
      "flexProps": {
          "alignSelf": "AUTO",
          "positionType": "RELATIVE",
          "flexGrow": 1
        },
      "borderColor": "#FFFFFF",
      "borderWidth": 0.5
    }
  }
}